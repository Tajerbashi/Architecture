@{
    ViewData["Title"] = "Home Page";
    var response = ViewBag.response;
}

<div class="container-fluid text-center">
    <div class="row d-flex flex-row justify-content-between" style="height:800px !important;">
        @foreach (var item in response)
        {
            <div class="bg-warning bg-gradient text-dark rounded-3 shadow px-2 m-2">@item.Id : @item.Name : @item.Family</div>
        }
    </div>
</div>
